#Ass-1
n=int(input())
for i in range(1,n+1):
    for j in range(1,i+1):
        print(j,end=' ')
    print()
    
 #Ass-2   
n=int(input())
for i in range(1,n+1):
    for j in range(1,i+1):
        print('*',end=' ')
    print()
    
#Ass-3
n=int(input())
c=0
for i in range(2,n+1):
    a=0
    for j in range(1,i+1):
        if i%j==0:
            a=a+1
    if a==2:
        c=c+1
print(c)
